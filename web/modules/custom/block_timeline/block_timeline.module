<?php

/**
 * @file
 * Contains block_timeline.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function block_timeline_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the block_timeline module.
    case 'help.page.block_timeline':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Block affichant une timeline') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function block_timeline_theme() {
  return [
    'block_timeline' => [
      'variables' => [
        'var' => [],
      ],
    ],
  ];
}
