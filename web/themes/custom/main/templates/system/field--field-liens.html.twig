{% for item in items %}
    <div class="row row-fichier-liens py-3 mx-3">
        <div class="col-lg-6 p-0">
            {{ item.content['#title'] }}
        </div>
        <div class="col-lg-6 p-0 text-right">
            <a href="{{ item.content['#url'] }}" target="_blank" class="txt-white">
              <i class="fas fa-eye txt-white"></i>
                Voir cette page
            </a>
        </div>
    </div>
{% endfor %}
